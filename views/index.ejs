<%- include("partials/header") %> <% blogs.forEach((blog)=>{ %>
<div>
  <h2><%= blog.title %></h2>
  <img src="<%= blog.image %>" />
  <span><%= blog.created.toDateString() %></span> </span>
  <p><%= blog.body.substring(0, 100) %>...</p>
  <a href="/blogs/<%= blog._id%>">Read more</a>
</div>
<% }) %> <%- include("partials/footer") %>
